#include <iostream>
#include <cstdio>
#include <cstring>
using namespace std;

int main()
{
    int i;
    int j;
    int input;
    char str[80];
    char numbers [18][20] = {
       
        "One", "1",
        "Two", "2",
        "Three","3",
        "Four", "4",
        "Five", "5",
        "Six", "6",
        "Seven", "7",
        "Eght", "8",
        "Nine", "9",
    };
    cout << "Введите число:";
    cin >> str;
    for(i=1; i < 18; i +=2)
    if (!strcmp(str,numbers[i])) {
    cout << numbers [i-1] << "\n";
break;
}

    return 0;
}
